<UserControl x:Class="AutonomousCars.Car"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AutonomousCars"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Width="800" Height="600">
    <UserControl.Resources>
        <local:MarginConverter x:Key="MarginConverter"></local:MarginConverter>
    </UserControl.Resources>
    <Grid>
        <Rectangle x:Name="MyRect" Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="40" Stroke="Black" VerticalAlignment="Top" Width="40">
            <Rectangle.Margin>
                <MultiBinding Converter="{StaticResource MarginConverter}">
                    <Binding Path="Lane" />
                    <Binding Path="Position" />
                </MultiBinding>
            </Rectangle.Margin>
        </Rectangle>

    </Grid>
</UserControl>
